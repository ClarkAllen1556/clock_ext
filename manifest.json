{
  "description": "In browser timepiece",
  "homepage_url": "http://www.theallenc.com",
  "manifest_version": 2,
  "name": "Clock",
  "permissions": [],
  "browser_action": {
    "default_icon": {
      "19": "button/geo-19.png",
      "38": "button/geo-38.png"
    },
    "default_title": "Whereami?",
    "size": 32
  },
 "content_scripts": [
  {
    "matches": [
      "*://*.mozilla.org/*"
    ],
    "js": [
      "./js/content.js"
    ]
  }],
  "background": {
    "page": "background.html"
  },
  "icons": {},
  "version": "1.0"
}