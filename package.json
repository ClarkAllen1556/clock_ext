{
  "name": "clock_ext",
  "version": "1.2.1",
  "description": "In-browser timepiece.",
  "homepage": "https://github.com/ClarkAllen1556/clock_ext",
  "main": "manifest.json",
  "author": "AClark",
  "license": "MIT",
  "private": false,
  "scripts": {
    "start": "web-ext -v run",
    "clean-build": "yarn clean && yarn build",
    "build": "tsc --project .",
    "clean": "rm -f js/*.js",
    "deep-clean": "rm -rf js/* && rm -rf build/*",
    "lint": "tslint -p .",
    "package": "yarn clean-build && web-ext build -a ./build -v -i ./ts ./node_modules yarn.lock tslint* .git* ./.git/",
    "wp": "yarn webpack --mode production"
  },
  "dependencies": {
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11"
  },
  "devDependencies": {
    "@ianwalter/web-ext-webpack-plugin": "^0.1.0",
    "css-loader": "^3.5.2",
    "file-loader": "^6.0.0",
    "html-loader": "^1.1.0",
    "html-webpack-plugin": "^4.2.0",
    "style-loader": "^1.1.4",
    "ts-loader": "^7.0.1",
    "tslint": "^6.1.1",
    "typescript": "^3.8.3",
    "web-ext-types": "^3.2.1"
  }
}
