{
  "name": "clock_ext",
  "version": "1.2.0",
  "description": "In-browser timepiece.",
  "homepage": "https://github.com/ClarkAllen1556/clock_ext",
  "main": "manifest.json",
  "author": "AClark",
  "license": "MIT",
  "private": false,
  "scripts": {
    "start": "web-ext -v run",
    "clean-build": "yarn clean && yarn build",
    "build": "tsc --project .",
    "clean": "rm -f js/*.js",
    "deep-clean": "rm -rf js/* && rm -rf build/*",
    "lint": "tslint -p .",
    "package": "web-ext build -a ./build -v -i ./ts/* ./node_modules yarn.lock tslint* .git* ./.git/"
  },
  "dependencies": {},
  "devDependencies": {
    "tslint": "^6.1.1",
    "web-ext-types": "^3.2.1"
  }
}
